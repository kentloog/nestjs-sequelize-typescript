{
    "name": "nest-sequelize-typescript",
    "version": "0.0.1",
    "description": "Nest + sequelize-typescript + JWT + Jest + Swagger",
    "keywords": [
        "nest",
        "nestjs",
        "nest.js",
        "sequelize",
        "sequelize-typescript",
        "orm",
        "nodejs",
        "node.js",
        "node",
        "typescript",
        "jwt",
        "jsonwebtoken",
        "passport",
        "swagger",
        "jest"
    ],
    "author": "Kent Loog",
    "license": "MIT",
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "db:migrate": "sequelize-cli db:migrate",
        "db:drop": "sequelize-cli db:drop",
        "db:create": "sequelize-cli db:create",
        "db:seed-dev": "sequelize-cli db:seed:all",
        "db:seed-prod": "sequelize-cli db:seed:all --seeders-path db/seeders-prod",
        "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed-dev && npm run db:seed-prod",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^11.0.0",
        "@nestjs/core": "^11.0.0",
        "@nestjs/passport": "^11.0.0",
        "@nestjs/platform-express": "^11.0.0",
        "@nestjs/swagger": "^11.0.0",
        "bcrypt": "^6.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "jsonwebtoken": "^9.0.2",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "sequelize": "^6.37.5",
        "sequelize-cli": "^6.6.2",
        "sequelize-typescript": "^2.1.6",
        "swagger-ui-express": "^5.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.0",
        "@types/bcrypt": "^6.0.0",
        "@types/express": "^5.0.0",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^24.0.0",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/sequelize": "^4.28.20",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^30.0.0",
        "prettier": "^3.4.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
